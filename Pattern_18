#include<bits/stdc++.h>
using namespace std;
int main()
{
    int n;
    cin>>n;
    int print=n;
    for(int row=1;row<=n;row++)
    {
        for(int col=1;col<=n;col++)
        {
          if(row==col)
          {
              cout<<"*";
          }
          else if(col==print)
          {
              cout<<"*";
          }
          else
          {
              cout<<" ";
          }
        }
        print--;
        cout<<endl;
    }
    return 0;
}
