#include<bits/stdc++.h>
using namespace std;
int main(){
    int n;
    cin>>n;
    int num=0;
    for(int i=1;i<=n;i++)
    {
         if(i%2==0)
            {
                num=10;
            }
            else
            {
                num=90;
            }
        for(int j=1;j<=n;j++)
        {
            if(i%2==0)
            {
                cout<<num<<" ";
                num+=10;
            }
            else
            {
                cout<<num<<" ";
                num-=10;
            }
        }
        cout<<endl;
    }
    return 0;
    
}
