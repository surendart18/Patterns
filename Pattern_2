#include<bits/stdc++.h>
using namespace std;
int main(){
    int n,Val=1;
    cin>>n;
    int Split=n/2;
    for(int i=1;i<=n;i++)
    {
        for(int j=1;j<=n;j++)
        {
            if(j<=Split)
            {
                cout<<Val++<<" ";
            }
            else
            {
                cout<<"* ";
            }
        }
        cout<<endl;
    }
    return 0;
}
