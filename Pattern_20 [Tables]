#include<bits/stdc++.h>
using namespace std;
int main()
{
    int num1=1,num2=1;
    cin>>num1;
    cin>>num2;
    int product=1;
    for(int row=1;row<=num1;row++)
    {
        product=row*num2;
        for(int col=1;col<=1;col++)
        {
            
            cout<<row<<" x "<<num2<<" = "<<product;
        }
        cout<<endl;
    }
    return 0;
}
